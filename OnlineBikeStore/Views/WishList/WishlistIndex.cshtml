@model List<OnlineBikeStore.Models.WishListViewModel>
@{
    ViewBag.Title = "WishlistIndex";
    Layout = null;
}
<div>
    <div class="d-flex justify-content-between mb-1">
        <span>
            
        </span>        
        
    </div>
    <div class="">
        @foreach (var item in Model)
        {
            <div class="content-row d-flex" onclick="product()">
                @if (item.image_url != null)
                {
                    <img src="@Url.Content(item.image_url)"  width="70" />
                }
                <div class="p-3">
                    @item.product_name
                    <br />
                    @item.price
                </div>
                <p style="text-align:end">Remove</p>
            </div>
        }
    </div>
</div>
<script>

</script>